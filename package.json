{
  "name": "brackets",
  "version": "0.0.0",
  "description": "Manage and participate in tournaments",
  "main": "index.js",
  "author": "Matthew Trager <mtrager20@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "env NODE_ENV='prod' nodemon index.js",
    "prestart": "npm run build",
    "prestart:dev": "npm run build:dev",
    "start:dev": "env NODE_ENV='server' nodemon index.js",
    "prebuild": "rimraf dist",
    "build": "webpack -p --env.prod",
    "prebuild:dev": "rimraf dist",
    "build:dev": "webpack --env.dev",
    "watch:dev": "npm run build:dev -- --watch",
    "dev": "webpack-dev-server --env.dev",
    "lint": "eslint src/js/**/*.jsx src/js/**/*.js webpack.config.babel.js",
    "test": "karma start",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "validate": "npm-run-all --parallel built test"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^7.4.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.9.4",
    "morgan": "^1.8.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "styled-components": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-standard": "^10.2.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
